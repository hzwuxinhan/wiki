{
  "name": "wiki",
  "displayName": "wiki",
  "description": "parse wiki from web and create mock file",
  "version": "0.0.1",
  "publisher": "hzwuxinhan",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:wiki.parse"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "wiki.parse",
        "title": "wiki: parse"
      }
    ],
    "configuration": {
      "properties": {
        "wiki.config": {
          "type": "object",
          "default": {},
          "description": "config file for wiki"
        }
      },
      "title": "wiki Configuration Settings"
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "icon": "icon.jpg",
  "dependencies": {
    "parsewiki": "^1.1.4"
  }
}