{
  "name": "wiki",
  "displayName": "wiki",
  "description": "parse wiki from web and create mock file",
  "version": "0.0.1",
  "publisher": "hzwuxinhan",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:wiki.parse",
    "onCommand:wiki.config"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "wiki.parse",
        "title": "wiki: parse"
      },
      {
        "command": "wiki.config",
        "title": "wiki: config"
      }
    ],
    "configuration": {
      "properties": {
        "wiki.password": {
          "type": "string",
          "description": "password for wiki"
        },
        "wiki.username": {
          "type": "string",
          "description": "username for wiki"
        },
        "wiki.mainUrl": {
          "type": "string",
          "description": "domain of wiki like:https://test.wiki.com"
        },
        "wiki.mockPath": {
          "type": "string",
          "description": "folder path for mock file,has default value:\"src/test/mock\""
        },
        "wiki.ftlPre": {
          "type": "string",
          "description": "prefix of ftl in wiki, has default value:\"src/main/webapp/WEB-INF/tmpl\""
        },
        "wiki.tddPre": {
          "type": "string",
          "description": "prefix of tdd in wiki, has default value:\"src/test/tdd\""
        }
      },
      "title": "wiki Configuration Settings"
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "icon": "icon.jpg",
  "dependencies": {
    "parsewiki": "^1.1.5"
  }
}