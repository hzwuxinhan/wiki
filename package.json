{
  "name": "wiki",
  "displayName": "wiki",
  "description": "parse wiki from web and create mock file",
  "version": "0.1.7",
  "publisher": "hzwuxinhan",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.wikiparse",
        "title": "wiki: parse "
      },
      {
        "command": "extension.wikiconfig",
        "title": "wiki: config 配置wiki"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.wikiparse",
          "group": "myGroup@1"
        },
        {
          "command": "extension.wikiconfig",
          "group": "myGroup@1"
        }
      ]
    },
    "configuration": {
      "properties": {
        "wiki.passWord": {
          "type": "string",
          "description": "password for wiki"
        },
        "wiki.userName": {
          "type": "string",
          "description": "username for wiki"
        },
        "wiki.pageUrl": {
          "type": "string",
          "description": "domain of wiki like:https://test.wiki.com"
        },
        "wiki.mockPath": {
          "type": "string",
          "description": "folder path for mock file,has default value:\"src/test/mock\""
        },
        "wiki.ftlPre": {
          "type": "string",
          "description": "prefix of ftl in wiki, has default value:\"src/main/webapp/WEB-INF/tmpl\""
        },
        "wiki.tddPre": {
          "type": "string",
          "description": "prefix of tdd in wiki, has default value:\"src/test/mock/tdd\""
        }
      },
      "title": "wiki Configuration Settings"
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "icon": "icon.png",
  "dependencies": {
    "@types/node": "^6.0.63",
    "babel-runtime": "^6.23.0",
    "cheerio": "^0.22.0",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "sync-exec": "^0.6.2",
    "vscode": "^1.0.3"
  }
}
